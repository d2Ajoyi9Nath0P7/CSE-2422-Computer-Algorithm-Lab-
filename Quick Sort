void fastIO()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
}
int partition(vector<int>&vec,int lb,int ub)
{
    int pivot = vec[lb];
    int start = lb;
    int end = ub;
    while(start < end)
    {
        while(vec[start] <= pivot)
        {
            start++;
        }
        while(vec[end] > pivot)
        {
            end--;
        }
        if(start < end)
        {
            swap(vec[start],vec[end]);
        }
    }
    swap(vec[end],vec[lb]);
    return end;//end position
}
void qs(vector<int>&vec,int lb,int ub)
{
    if(lb < ub)
    {
        int index = partition(vec,lb,ub);
        qs(vec,lb,index-1);
        qs(vec,index+1,ub);
    }
}
void solve()
{
    int n;
    cin>>n;
    vi vec(n);
    for(int i = 0 ;  i < n ; i++)
    {
        cin>>vec[i];
    }
    int lb = 0;
    int ub = n-1;
    qs(vec,lb,ub);
    for(auto i : vec)
    {
        cout<<i<<" ";
    }
}

int main()
{
    fastIO();
    solve();
}
